{% extends '::base.html.twig' %}

{% block content %}
    <div class="col-xs-12 bottom-15">

        {% for type, messages in app.session.flashBag.all %}
            {% for message in messages %}
                <div class="{{ type }}">
                    {{ message }}
                </div>
            {% endfor %}
        {% endfor %}

        {% block fos_user_content %}{% endblock %}
    </div>
{% endblock %}
